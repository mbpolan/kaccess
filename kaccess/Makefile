####### Build rules

all: Makefile $(TARGET)

$(TARGET):  $(UICDECLS) $(OBJECTS) $(OBJMOC)  
	$(LINK) $(LFLAGS) -o $(TARGET) $(OBJECTS) $(OBJMOC) $(OBJCOMP) $(LIBS)

mocables: $(SRCMOC)
uicables: $(UICDECLS) $(UICIMPLS)

$(MOC): 
	( cd $(QTDIR)/src/moc && $(MAKE) )

Makefile: kaccess.pro  /usr/lib/qt3/mkspecs/default/qmake.conf /usr/lib/qt3/lib/libqt-mt.prl
	$(QMAKE) -o Makefile kaccess.pro
qmake: 
	@$(QMAKE) -o Makefile kaccess.pro

dist: 
	@mkdir -p .tmp/kaccess && $(COPY_FILE) --parents $(SOURCES) $(HEADERS) $(FORMS) $(DIST) .tmp/kaccess/ && ( cd `dirname .tmp/kaccess` && $(TAR) kaccess.tar kaccess && $(GZIP) kaccess.tar ) && $(MOVE) `dirname .tmp/kaccess`/kaccess.tar.gz . && $(DEL_FILE) -r .tmp/kaccess

